{
  "name": "react-template",
  "version": "1.0.9",
  "description": "这是一个最新版的react模板,会一直维护更新,因为没有类似vue-cli那样的工具所以自己建了个",
  "engines": {
    "npm": ">=8"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "dev": "vite --open --force",
    "build:vite": "npm run remove-dist && vite build --mode production --emptyOutDir",
    "start-message": "node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel  open:src lint:watch test:watch",
    "open:src": " webpack serve --mode development",
    "open:dist": "node tools/distServer.js",
    "lint": "esw --ext .tsx,.jsx,.js,.ts src/ tools/ --fix --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint",
    "build": "node tools/build.js  && npm run open:dist",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "keep_wan",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "echarts": "^5.5.0",
    "history": "^5.3.0",
    "js-cookie": "^3.0.5",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/eslint-parser": "^7.24.6",
    "@babel/plugin-syntax-async-generators": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-arrow-functions": "^7.24.6",
    "@babel/plugin-transform-async-generator-functions": "^7.24.6",
    "@babel/plugin-transform-async-to-generator": "^7.24.6",
    "@babel/plugin-transform-class-properties": "^7.24.6",
    "@babel/plugin-transform-json-strings": "^7.24.6",
    "@babel/plugin-transform-optional-chaining": "^7.24.6",
    "@babel/plugin-transform-react-constant-elements": "^7.24.6",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/runtime": "^7.24.6",
    "@loadable/component": "^5.16.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.13",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browser-sync": "^3.0.2",
    "chalk": "^4.1.2",
    "connect-history-api-fallback": "^2.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.37.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "7.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-watch": "^8.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "npm-run-all": "^4.1.5",
    "open": "^10.1.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-pxtorem": "^6.1.0",
    "react-refresh": "^0.14.2",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.7",
    "sass": "^1.77.3",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "unplugin-auto-import": "^0.17.6",
    "vite": "^5.2.12",
    "vite-plugin-compression2": "^1.1.1",
    "vite-plugin-cdn2": "^1.1.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "browserslist": [
    "last 15 versions",
    "> 0.00001%",
    "maintained node versions",
    "not dead"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    }
  },
  "overrides": {
    "@pmmmwh/react-refresh-webpack-plugin": {
      "webpack-dev-server": "^5.0.4"
    }
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "webpack",
    "redux",
    "boilerplate",
    "starter",
    "react-template",
    "react-version18"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/whevether/react-template.git"
  }
}
